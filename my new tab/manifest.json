{
  "name": "my-new-tab-page",
  "manifest_version": 3,
	"icons": {
		"16": "images/16.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
  "description": "My new tab page.",
  "version": "1.0.0",
  
  "permissions": [
        "storage"
    ], 
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
  
}